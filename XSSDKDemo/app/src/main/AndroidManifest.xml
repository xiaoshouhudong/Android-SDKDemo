<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xshd.xssdkdemo">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.xshd.sdk.ui.activity.users.XSLoginActivity" />
        <activity android:name="com.xshd.sdk.ui.activity.users.XSRegisterActivity" />
        <activity android:name="com.xshd.sdk.ui.activity.users.XSUpdatePasswordActivity" />
        <activity android:name="com.xshd.sdk.ui.activity.users.XSUserCenterActivity" />
        <activity android:name="com.xshd.sdk.ui.activity.users.XSPhoneActivity" />
        <activity android:name="com.xshd.sdk.ui.activity.users.XSResePswActivity" />
        <activity android:name="com.xshd.sdk.ui.activity.other.XSAboutActivity" />
        <activity android:name="com.xshd.sdk.ui.activity.other.XSWebActivity" />
        <activity android:name="com.xshd.sdk.ui.activity.other.XSHelpActivity" />
        <activity android:name="com.xshd.sdk.ui.activity.other.XSExitActivity"
            android:theme="@style/xs_main_dialog_theme" />
        <activity android:name="com.xshd.sdk.ui.activity.pay.XSPayActivity" />
    </application>

</manifest>